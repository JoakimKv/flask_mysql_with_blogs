
# .env.example


# Example environment file for Flask + MySQL project.
# Copy this file to .env and replace values with your own credentials.

# Root MySQL user (for admin/maintenance, not used by Flask).
MY_SQL_GIT_USERNAME=my_root_username
MY_SQL_GIT_PASSWORD=my_root_password

# Application MySQL user (used by Flask).
MY_SQL_GIT_JK_USERNAME=db_user_username
MY_SQL_GIT_JK_PASSWORD=db_user_password
